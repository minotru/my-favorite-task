{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-plusplus": "off",
    "func-names": "off",
    "no-multi-assign": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "prop-types": "off"
  }
}
